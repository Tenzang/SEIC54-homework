<%= form_for @song do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <%= f.label :language%>
    <%= f.text_field :language%>
    <%= f.label :genre%>
    <%= f.text_field :genre%>
    <%= f.label :length%>
    <%= f.text_field :length%>
    <%= f.label :band_id %>
    <%= f.select :band_id, Band.pluck(:name, :id), :include_blank => true %>
    
    <%= f.submit %>
    <% end %>