
<h1> Edit Artist </h1>
<form action="/artists/<%= @artist.id %>" method='post'>
    <label>
        Name: 
        <input name="name" required value="<%= @artist.name%>">
    </label>

    <label>
        Label:
        <input name="label" value="<%= @artist.label%>">
    </label>

    <label>
        Image:
        <input name="image" type="url" value="<%= @artist.image%>">
    </label>
    
    <button> Update </button>
</form>